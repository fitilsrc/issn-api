{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/issn-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/issn-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/issn-api",
  "projects": {
    "issn-api": {
      "type": "application",
      "root": "apps/issn-api",
      "entryFile": "main",
      "sourceRoot": "apps/issn-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/issn-api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "persons": {
      "type": "application",
      "root": "apps/persons",
      "entryFile": "main",
      "sourceRoot": "apps/persons/src",
      "compilerOptions": {
        "tsConfigPath": "apps/persons/tsconfig.app.json"
      }
    },
    "presence": {
      "type": "application",
      "root": "apps/presence",
      "entryFile": "main",
      "sourceRoot": "apps/presence/src",
      "compilerOptions": {
        "tsConfigPath": "apps/presence/tsconfig.app.json"
      }
    }
  }
}